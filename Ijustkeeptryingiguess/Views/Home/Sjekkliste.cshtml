@model CheckListViewModel

<h2>Service Checklist for Vinsj</h2>

<form asp-action="Save" method="post">
    <div class="form-group">
        <label for="SerialNumber">Serie nr:</label>
        <input type="text" id="SerialNumber" name="SerialNumber" class="form-control" value="@Model.SerialNumber" />
    </div>

    <div class="form-group">
        <label for="Type">Type:</label>
        <input type="text" id="Type" name="Type" class="form-control" value="@Model.Type" />
    </div>

    <br />

    <h3>Mekanisk</h3>
    <table class="table">
        <thead>
            <tr>
                <th>Sjekkpunkt</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Sjekk clutch lameller for slitasje</td>
                <td>
                    <select id="ClutchLamellStatus" name="ClutchLamellStatus">
                        <option value="OK">OK</option>
                        <option value="Bør skiftes">Bør skiftes</option>
                        <option value="Defekt">Defekt</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td>Sjekk bremser. Bånd/Pal</td>
                <td>
                    <select id="BremserStatus" name="BremserStatus">
